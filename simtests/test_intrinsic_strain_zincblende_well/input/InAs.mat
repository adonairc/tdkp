# tdkpconfdef: material_zincblende.cnf
#
# InAs material material parameters 
#
# the values were taken from Table K.2
# in Chuang 'Physics of Optoelectronic 
# devices', p. 709, John Wiley & Sons 1995  
#
# the material parameters should be at 0K
#

conduction_band_edge    = 0.42
valence_band_edge       = 0.0

electron_effective_mass = 0.02226

luttinger_parameter_1   = 20.4
luttinger_parameter_2   = 8.3
luttinger_parameter_3   = 9.1

optical_matrix_element  = 22.0
bandgap                 = 0.42
spin_orbit_splitting    = 0.38


lattice_constant     = 6.05
#lattice_constant = 5.65031

elastic_constant_C11 = 83.0e9
elastic_constant_C12 = 45.3e9
elastic_constant_C44 = 39.6e9
